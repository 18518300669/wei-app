.container{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;

}
.map{
  flex: 1;
}
.map map{
  width: 100%;
  height: 100%;
}
.tabBar{
  height: 88rpx;
  width: 100%;
  display: flex;
}
.tabBar view{
  flex: 1;
  text-align: center;
  line-height: 88rpx;
  color: #fff;
  font-weight: 100;
}
.tabBar view:nth-child(1){
  background: #ff9700;
}
.tabBar view:nth-child(2){
  background: #03bbd5;
}